/**app.wxss**/
page {
  background-color: rgb(245,245,248);
}

.loading{
  width: 100vw;
  height: 70vh;

  display:flex;
  justify-content: center;
  align-items: center;
}
.loader-container {  
  height: 150px;  
  width: 200px;  
  display: flex;  
  justify-content: center;  
  align-items: center;
}
.loader {  
  width: 50px;  
  height: 50px;}
.one {  
  background-color: #17C3B2;  
  border-radius: 50%;  
  animation: loader1Animation 1s ease-in-out infinite;  margin-top: 50px;}
.one:before {  
  content: "";  
  position: absolute;  
  top: 5px;  
  left: 19px;  
  width: 12px;  
  height: 12px;  
  border-radius: 50%;  
  background-color: #fff;}
@keyframes 
loader1Animation {  0% {    transform: rotate(0deg);  }  100% {    transform: rotate(360deg);  }}